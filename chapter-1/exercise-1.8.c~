//1.8:Write a program to count blanks, tabs, and new lines

#include <stdout.h>

int main{
    char c;
    int count = 0;
    
    c = getchar();
    while(c != EOF){
	if(c == ' ' || c == \t || c == \n){
	    count++;
	}
    }


    printf("The number of blanks, tabs & newlines: %d", count);
    
    
}
